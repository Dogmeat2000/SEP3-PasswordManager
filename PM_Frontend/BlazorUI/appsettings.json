{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "BaseAddress": "https://your-base-address.com/",
    "LoadBalancerUrl": "http://localhost:8080"
  },
  "ud-kommenteret": {
    "Kestrel": {
      "Endpoints": {
        "Https": {
          "Url": "https://localhost:5001",
          "Certificate": {
            "Path": "path/to/certificate.pfx",
            "Password": "your_certificate_password"
          }
        }
      }
    }
  }
}
